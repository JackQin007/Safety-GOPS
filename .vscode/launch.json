{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run quadrotor/rl_experiment.py",
            "type": "python",
            "program": "${workspaceFolder}/quadrotor/rl_experiment.py",
            "args": [
                "--algo",
                "ppo",
                "--task",
                "quadrotor",
                "--overrides",
                "quadrotor/env/quadrotor_2D/quadrotor_2D_track.yaml",
                "quadrotor/env/quadrotor_2D/ppo_quadrotor_2D.yaml",
                "--kv_overrides",
                "algo_config.training=False"
            ],
 
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopOnEntry": false,
            "request": "launch",
        }
    ]
}
